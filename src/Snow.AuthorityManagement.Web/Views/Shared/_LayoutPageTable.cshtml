<!DOCTYPE html>

<html>
<head>
    <!-- 让Https页面支持Http请求 -->
    <meta http-equiv="Content-Security-Policy" content="upgrade-insecure-requests">
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <environment include="Development">
        <link href="~/lib/twitter-bootstrap/css/bootstrap.min.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/lib/toastr.js/toastr.min.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/lib/font-awesome/css/all.min.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/lib/ladda/ladda-themeless.min.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/lib/bootstrap-table/bootstrap-table.min.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/lib/bootstrap-daterangepicker/daterangepicker.min.css" rel="stylesheet" asp-append-version="true" />
    </environment>
    <environment exclude="Development">
        <link href="~/css/bundle/frame.min.css" rel="stylesheet" asp-append-version="true" />
    </environment>
    @RenderSection("styles", required: false)
</head>
<body>
    <!--页面加载end-->
    <div class="modal fade" id="modifyModal" tabindex="-1" role="dialog" aria-labelledby="modifyModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <input type="hidden" id="operateType" />
            <div class="modal-content" id="modifyContent">
            </div>
        </div>
    </div>

    @RenderBody()

    <table id="tb-body" data-bind="bootstrapTable :$root" data-filter-control="true"></table>

    <environment include="Development">
        <!-- jquery -->
        <script src="~/lib/jquery/jquery.js"></script>
        <!-- jquery-validate -->        @*
            <script src="/js/bundle/jqueryValidate.min.js"></script>*@
        <script src="~/lib/jquery-validate/jquery.validate.min.js"></script>
        <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
        @*<script src="~/lib/jquery-validate/jquery.unobtrusive-ajax.min.js"></script>*@

        <!-- bootstrap -->
        <script src="~/lib/twitter-bootstrap/js/bootstrap.bundle.min.js"></script>
        <!-- ladda -->
        <script src="~/lib/ladda/spin.min.js"></script>
        <script src="~/lib/ladda/ladda.min.js"></script>
        <!-- daterangepicker -->
        <script src="~/lib/bootstrap-daterangepicker/moment.min.js"></script>
        <script src="~/lib/bootstrap-daterangepicker/daterangepicker.min.js"></script>

        <!-- bootstrap-iconpicker -->
        @*
            <script src="~/js/bundle/bootstrapIconpicker.min.js"></script>*@
        @*
            <script src="~/lib/bootstrap-iconpicker/js/bootstrap-iconpicker-iconset-all.min.js"></script>
            <script src="~/lib/bootstrap-iconpicker/js/bootstrap-iconpicker.min.js"></script>*@

        <!-- bootstrap-table -->
        @*
            <script src="~/js/bundle/bootstrapTable.min.js"></script>*@
        <script src="~/lib/bootstrap-table/bootstrap-table.min.js"></script>
        <script src="~/lib/bootstrap-table/locale/bootstrap-table-zh-CN.min.js"></script>
        @*
            <script src="~/lib/bootstrap-table/extensions/fixed-columns/bootstrap-table-fixed-columns.min.js"></script>*@

        <!-- 导出 -->
        @*
            <script src="~/js/bundle/bootstrapTableExport.min.js"></script>*@
        @*
            <script src="~/lib/bootstrap-table/extensions/export/bootstrap-table-export.js"></script>
            <script src="~/lib/tableExport.jquery.plugin/libs/FileSaver/FileSaver.min.js"></script>
            <!-- Excel 2007+ XML Format -->
            <script src="~/lib/tableExport.jquery.plugin/libs/js-xlsx/xlsx.core.min.js"></script>
            <!-- PDF -->
            <script src="~/lib/tableExport.jquery.plugin/libs/jsPDF/jspdf.min.js"></script>
            <script src="~/lib/tableExport.jquery.plugin/libs/jsPDF-AutoTable/jspdf.plugin.autotable.js"></script>
            <!-- PNG -->
            <script src="~/lib/tableExport.jquery.plugin/libs/html2canvas/html2canvas.min.js"></script><!-- Excel 2007+ XML Format -->
            <script src="~/lib/tableExport.jquery.plugin/libs/js-xlsx/xlsx.core.min.js"></script>*@
        <!-- PDF -->
        @*
            <script src="~/lib/tableExport.jquery.plugin/libs/jsPDF/jspdf.min.js"></script>*@
        @*
            <script src="~/lib/tableExport.jquery.plugin/libs/jsPDF-AutoTable/jspdf.plugin.autotable.js"></script>*@
        <!-- PNG -->
        @*
            <script src="~/lib/tableExport.jquery.plugin/libs/html2canvas/html2canvas.min.js"></script>*@
        @*
            <script src="~/lib/tableExport.jquery.plugin/tableExport.min.js"></script>*@

        <!-- 弹框 -->
        <script src="~/lib/toastr.js/toastr.min.js"></script>
        <!--<script src="https://cdnjs.cloudflare.com/ajax/libs/bootbox.js/4.4.0/bootbox.min.js"></script>-->
        <script src="~/lib/bootbox.js/bootbox.all.min.js"></script>
        <script src="~/lib/bootbox.js/locales/zh_CN.js"></script>

        <!-- select2 -->
        @*
            <script src="~/js/bundle/select2.min.js"></script>*@
        <script src="~/lib/select2/js/select2.full.min.js"></script>
        <script src="~/lib/select2/js/i18n/zh-CN.js"></script>
        <!-- 自定义 -->
        @*<script src="~/js/jquery.formPopulate.js"></script>*@
        <script src="~/js/anc.js"></script>
        <script src="~/js/dataInit.js"></script>
        <script src="~/js/common.js"></script>
        <script src="~/js/frame-common.js"></script>
        <script src="~/js/main.js"></script>
    </environment>
    <environment exclude="Development">
        @*
            <script src="~/js/bundle/frame.min.js"></script>*@
    </environment>
    @RenderSection("scripts", false)
    <script type="text/javascript">
         absoluteUrl = '@ViewBag.AbsoluteUrl';
        title = '@ViewBag.Title';
    </script>
</body>
</html>